<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Native_Language__c</fullName>
    <description>This  filed auto populate the Native  language according to the state</description>
    <externalId>false</externalId>
    <formula>CASE(
  State__c,
  &quot;Maharashtra&quot;, &quot;Marathi&quot;,
  &quot;Uttar Pradesh&quot;, &quot;Hindi&quot;,
  &quot;Madhya Pradesh&quot;, &quot;Hindi&quot;,
  &quot;Rajasthan&quot;, &quot;Hindi&quot;,
  &quot;Gujarat&quot;, &quot;Gujarati&quot;,
  &quot;Tamil Nadu&quot;, &quot;Tamil&quot;,
  &quot;Unknown&quot;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Native langugae</inlineHelpText>
    <label>Native Language</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
